{
  "name": "size-project",
  "displayName": "Size Project",
  "description": "Shows the total number of non-empty lines of code in your project",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "publisher": "tuttodev",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#2c2c2c",
    "theme": "dark"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "size-project.calculateSize",
        "title": "Calculate Project Size"
      }
    ]
  },
  "scripts": {
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.17.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tuttodev/size-project-vscode-extension"
  },
  "keywords": [
    "size",
    "project",
    "lines of code",
    "statistics"
  ],
  "author": "tuttodev",
  "license": "MIT"
}
